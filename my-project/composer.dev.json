{
    "name": "thusitha/my-project",
    "description": "composer project development",
    "keywords": [
        "composer",
        "project",
        "development"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.mydomain.com",
            "options": {
                "ssl": {
                    "verify_peer": false
                }
            }
        }
    ],
    "require": {
        "monolog/monolog": "^2.0",
        "thusitha/composer-plugin": "dev-main",
        "thusitha/php-plugin": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "dev": [
            "echo 'Running development build'",
            "composer install"
        ]
    }
}